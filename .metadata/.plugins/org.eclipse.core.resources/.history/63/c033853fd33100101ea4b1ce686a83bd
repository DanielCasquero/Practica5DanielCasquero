package clases;

public class Tecnico {
	
	private String nombre;
	private int edad;
	private String ocupacion;
	private int aniosExperiencia;
	
	public Tecnico(String nombre, int edad, String ocupacion) {
		super();
		this.nombre = nombre;
		this.edad = edad;
		this.ocupacion = ocupacion;
	}
	
	
}
